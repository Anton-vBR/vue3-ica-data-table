{
  "name": "vue3-ica-data-table",
  "author": "anton-vbr",
  "private": false,
  "version": "0.0.3",
  "description": "Just another data table component for Vue3",
  "type": "module",
  "types": "./types/main.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "types"
  ],
  "homepage": "https://github.com/Anton-vBR/vue3-ica-data-table",
  "main": "./dist/vue3-ica-data-table.umd.js",
  "module": "./dist/vue3-ica-data-table.es.js",
  "exports": {
    ".": {
      "types": "./types/main.d.ts",
      "import": "./dist/vue3-ica-data-table.es.js",
      "require": "./dist/vue3-ica-data-table.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix 'src/**/*.{vue,html,css,js,ts}'",
    "format": "npx prettier . --write"
  },
  "keywords": [
    "vue3",
    "data-table-component",
    "data-table",
    "vue3-component",
    "vue-component"
  ],
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "prettier": "3.0.3",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
